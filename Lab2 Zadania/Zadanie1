#include <iostream>
using namespace std;

int main()
{
    bool is_prime = true;
    int x, y;
    cout << "Podaj x: ";
    cin >> x;
    cout << "Podaj y: ";
    cin >> y;
    if (x > y || x <= 0)
    {
        cout << "NieprawidÅ‚owy zakres";
        return 0;
    }
    for (int i = x; i <= y; i++)
    {
        if (i == 1) continue;
        for (int j = 2; j < i / 2 + 1; j++)
        {
            if (i % j == 0)
            {
                is_prime = false;
                break;
            }
        }
        if (is_prime)
        {
            cout << i << endl;
        }
        is_prime = true;
    }
}
